{
  "name": "backend_nodejs",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "start:dev": "ENV=DEVELOPMENT node dist/app.js",
    "watch:dev": "ENV=DEVELOPMENT tsx watch --inspect=0.0.0.0:9229 src/app.ts",
    "prisma:generate": "npx prisma generate",
    "prisma:deploy:dev": "npx dotenv -e .env.migration.development -- prisma migrate dev",
    "prisma:deploy:prod": "npx dotenv -e .env.migration.production -- prisma migrate dev"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "ts-enum-util": "^4.1.0",
    "@prisma/client": "^6.9.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.14.1",
    "pg-native": "^3.3.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.24.2",
    "luxon": "^3.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.6.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.13",
    "dotenv-cli": "^8.0.0",
    "prisma": "^6.9.0",
    "tsx": "^4.19.4",
    "typescript": "^5.6.3"
  }
}
